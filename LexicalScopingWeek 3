   makeCacheMatrix <- function(x = matrix()){
   inversssss <- NULL
   set <- function(y){
   x <<- y
   inversssss <<- NULL
  }
  get <- function() {x}
  setInverse <- function(inverse) {inversssss <<- inverse}
  getInverse <- function() {inversssss}
  list(set = set, get = get, setInverse = setInverse, getInverse = getInverse)
}

cacheSolve <- function(x, ...){
 invsssss <- x$getInverse() 
 if(!is.null(inversssss)){
 message("getting cached data")
 return(inversssss)  
  }
  mat <- x$get()
     inversssss <- solve(mat, ...)
     x$setInverse(inversssss)
     inversssss
}
