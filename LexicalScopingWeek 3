            ThiscanMakeCacheMatrix <- function(x = matrix()){


          inv <- NULL
  
   set <- function(y){
  
        x <<- y
    
           inv <<- NULL
    
  }
  
  
                         get <- function() {x}
  
                 setInverse <- function(inverse) {inv <<- inverse}
  
  
            gettoInverse <- function() {inv}
  
  
  list(set = set, get = get, setInverse = setInverse, gettoInverse = gettoInverse)
  
  
}



cacheSolve <- function(x, ...){

  inv <- x$gettoInverse()
  
        if(!is.null(inv)){
  
                message("get cached to  get data")
    
                       return(inv)
    
    
    
    
  }
  
  mat <- x$get()
  
      inv <- solve(mat, ...)
  
        x$setInverse(inv)
  
  
                     inv
  
  
  
  
}
